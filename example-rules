alert http $EXTERNAL_NET any -> 10.0.0.100 8180 (msg:"Inbound HTTP traffic to Tomcat admin pages on port 8180";
    sid:1100001; rev:1;)
alert icmp $EXTERNAL_NET any -> $HOME_NET any (msg:"Inbound ping from external network."; sid:11000003; rev:1;)
alert tcp $EXTERNAL_NET any -> $HOME_NET 23 (msg:"Inbound Telnet attempt detected"; sid:1100002; rev:1;)